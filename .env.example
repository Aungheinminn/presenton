# IMPORTANT: Copy this file to .env in the project root directory
# The .env file should be placed in the root directory, NOT in servers/fastapi or servers/nextjs
# When using start.js, all environment variables will be loaded from the root .env file

# LLM Configuration
# Options: openai, google, anthropic, ollama, custom
LLM=openai

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o

# Google Gemini Configuration
# GOOGLE_API_KEY=your_google_api_key_here
# GOOGLE_MODEL=models/gemini-2.0-flash

# Anthropic Claude Configuration
# ANTHROPIC_API_KEY=your_anthropic_api_key_here
# ANTHROPIC_MODEL=claude-3-5-sonnet-20241022

# Ollama Configuration (for local models)
# OLLAMA_URL=http://localhost:11434
# OLLAMA_MODEL=llama3.2:3b

# Custom OpenAI-Compatible API
# CUSTOM_LLM_URL=http://your-custom-endpoint
# CUSTOM_LLM_API_KEY=your_custom_api_key
# CUSTOM_MODEL=your_model_name
# TOOL_CALLS=true
# DISABLE_THINKING=false

# Image Generation Configuration
# Options: dall-e-3, gpt-image-1.5, gemini_flash, nanobanana_pro, pexels, pixabay, comfyui
IMAGE_PROVIDER=dall-e-3

# Set to true to disable image generation
# DISABLE_IMAGE_GENERATION=false

# Image Provider API Keys (based on IMAGE_PROVIDER choice)
# DALL_E_3_QUALITY=standard  # Options: standard, hd
# GPT_IMAGE_1_5_QUALITY=medium  # Options: low, medium, high
# PEXELS_API_KEY=your_pexels_api_key
# PIXABAY_API_KEY=your_pixabay_api_key

# ComfyUI Configuration (if using comfyui as IMAGE_PROVIDER)
# COMFYUI_URL=http://your-comfyui-server
# COMFYUI_WORKFLOW={"workflow": "json"}

# Application Settings
CAN_CHANGE_KEYS=true
DISABLE_ANONYMOUS_TELEMETRY=true

# Application data directory
APP_DATA_DIRECTORY=./app_data

# Temporary Directory for screenshots and intermediate files
TEMP_DIRECTORY=./app_data/temp

# Next.js URL (for local development)
NEXTJS_URL=http://localhost:3000

# Web Search (for OpenAI, Google, and Anthropic)
# WEB_GROUNDING=false

# Database Configuration (optional)
# DATABASE_URL=sqlite:///app_data/presenton.db

# Extended Reasoning (experimental)
# EXTENDED_REASONING=false
